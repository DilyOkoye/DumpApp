    <script>

        $('#btnCoreBanking').click(function () {
            var hostUrl = $('#serverIp').val()
            var port = $('#serverPort').val()

        $.ajax({
            url: $("#url_getDetails").val(),
            dataType: 'json',
            type: 'POST',
            cache: false,
            dataType: 'json',
            data: { hostUrl: hostUrl, port: port },

            success: function (data) {

                if (data != null)
                {
                    if (data == true)
                    {
                        alert("Connection to " +  hostUrl + " On Port " + port + " Successfull ");


                    }
                    else
                    {
                        alert("Failed to Connect to " + hostUrl + " On Port " + port);

                    }
                }
                else {
                    alert("Failed to Connect to " + hostUrl + " On Port " + port);
                  
                }
            },

            error: function (xhr, ajaxOptions, thrownError) {
               
                return false;
            }

        });
        });

        $('#btnCoreSmtp').click(function ()
        {
            var hostUrl = $('#smtpIp').val()
            var port = $('#smtpPort').val()

            $.ajax({
                url: $("#url_getDetails").val(),
                dataType: 'json',
                type: 'POST',
                cache: false,
                dataType: 'json',
                data: { hostUrl: hostUrl, port: port },

                success: function (data) {

                    if (data != null)
                    {
                        if (data == true)
                        {
                            alert("Connection to " + hostUrl + " On Port " + port + " Successfull");


                        }
                        else {
                            alert("Failed to Connect to " + hostUrl + " On Port " + port);

                        }
                    }
                    else {
                        alert("Failed to Connect to " + hostUrl + " On Port " + port);

                    }
                },

                error: function (xhr, ajaxOptions, thrownError) {

                    return false;
                }

            });
        });

    </script>
